<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity ParameterType.
    -->
    <changeSet id="20231018155825-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="parameter_id"
                                 baseTableName="parameter_type"
                                 constraintName="fk_parameter_type__parameter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="parameter"
                                 />

        <addForeignKeyConstraint baseColumnNames="parameter_type_definition_id"
                                 baseTableName="parameter_type"
                                 constraintName="fk_parameter_type__parameter_type_definition_id"
                                 referencedColumnNames="id"
                                 referencedTableName="parameter_type_definition"
                                 />
    </changeSet>
</databaseChangeLog>
